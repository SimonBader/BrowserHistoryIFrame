@{
    ViewBag.Title = "Home Page";
}

<iframe id="foo" src="@Url.Action("Index", "Child")" name="child"></iframe>
<script>
    var url = 'http://www.google.com'
    $('#foo').load(function (event) {
        var element = event.target;
        element.onunload(function() {
            element.contentWindow.historey.replaceState(null, null, url);
        });
    });
</script>